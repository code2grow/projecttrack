<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.jia.ptrack.domain"
	default-access="field">

	<class name="Status">
	
		<id name="id" type="int">
			<generator class="native" />
		</id>

		<property name="name" />

		<many-to-one name="rejectionStatus" cascade="all" />

		<many-to-one name="approvalStatus" cascade="all" />

		<property name="initialState" />

		<property name="finalState" />

		<property name="role"
			type="org.jia.ptrack.domain.hibernate.RoleTypeUserType" />

	</class>

</hibernate-mapping>

